{
  "name": "round-robin-chat",
  "private": true,
  "engines": {
    "node": "18"
  },
  "main": "server/index.ts",
  "scripts": {
    "start": "node built/index.js",
    "watch": "nodemon --watch server -e ts -x \"ts-node -r dotenv/config -P server/tsconfig.json server/index.ts\"",
    "build:server": "tsc -p server",
    "build:client": "webpack --config server/webpack/webpack.prod.ts",
    "build": "npm run build:server && npm run build:client",
    "check:server": "tsc -p server --noEmit",
    "check:client": "tsc -p client --noEmit",
    "check": "npm run check:server && npm run check:client",
    "eslint": "eslint .",
    "eslint:server": "eslint ./server",
    "eslint:client": "eslint ./client",
    "lint": "npm run eslint"
  },
  "dependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.10",
    "autoprefixer": "^10.4.14",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "express": "^4.18.2",
    "express-promise-router": "^4.1.1",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "morgan": "^1.10.0",
    "openai": "^3.3.0",
    "postcss": "^8.4.26",
    "postcss-loader": "^7.3.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "socket.io": "^4.7.1",
    "socket.io-client": "^4.7.1",
    "tailwindcss": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "uuid": "^9.0.0",
    "webpack": "^5.88.2",
    "webpack-dev-middleware": "^6.1.1",
    "webpack-hot-middleware": "^2.25.4",
    "webpack-merge": "^5.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "@babel/preset-env": "^7.22.9",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@types/uuid": "^9.0.2",
    "@types/webpack-hot-middleware": "^2.25.6",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "webpack-cli": "^5.1.4"
  }
}
